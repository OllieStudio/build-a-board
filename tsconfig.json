{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny":false,
    "resolveJsonModule":true,
    "esModuleInterop": true,
    "target": "es5",
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
